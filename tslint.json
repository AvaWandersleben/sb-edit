{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": [
      true,
      "match-declaration-order-only"
    ],
    "no-shadowed-variable": false,
    "prefer-const": false,
    "max-classes-per-file": false,
    "ordered-imports": false
  }
}